generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url = env("DATABASE_URL")
}

model Cluster {
    id String @db.VarChar(100)
    secret String @db.VarChar(100)
    @@id([id])
}

model DedicatedStorage {
    resourceId String @db.VarChar(100)
    clusterId String @db.VarChar(100)
    ownerId String @db.VarChar(100)
    amount Int
    @@id([resourceId, clusterId])
}
